    "            domain=dict(x=[0, 1], y=[0, 1]),\n",
    "        )\n",
    "    )\n",
    "    fig.update_layout(\n",
    "        paper_bgcolor=\"darkred\",\n",
    "        font_color=\"white\",\n",
    "        margin=dict(l=0, r=0, b=0, t=0, pad=0),\n",
    "        height=height,\n",
    "    )\n",
    "    return fig"
