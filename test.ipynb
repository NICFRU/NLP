    "    fig = go.Figure(\n",
    "        go.Indicator(\n",
    "            mode=\"number\",\n",
    "            value=hate_percent,\n",
    "            title=title,\n",
    "            number=dict(suffix=\"%\", valueformat=\".2f\"),\n",
